@model IEnumerable<TodoList.Models.ApplicationUser>
@{
    ViewBag.Title = "ShowManagers";
}

<div class="row">

    <div class="col-md-4">
        <h2>Show Managers</h2>
        <ul>
            <li class="list-group-item active">
                All Managers
            </li>
            @foreach (var user in Model)
            {
                <li class="list-group-item">@user.UserName</li>
            }
        </ul>
    </div>
</div>